        <nagEmailAddress>nick@atlassian.com</nagEmailAddress>

        <sourceDirectory>src/java</sourceDirectory>
        <resources>
            <resource>
                <directory>src/etc/</directory>
                <include>atlassian-plugin.xml</include>
                <include>**/*.vm</include>
            </resource>
        </resources>
        <!--
        <resources>
            <resource>
                <directory>src/etc/java</directory>
                <includes>**/*.*</includes>
            </resource>
        </resources>
		-->
        <unitTestSourceDirectory>src/test/java</unitTestSourceDirectory>
        <unitTest>
            <includes>
                <include>**/Test*.java</include>
                <include>**/*TestCase.java</include>
            </includes>
            <excludes>
                <exclude>**/ConfluenceTestCase.java</exclude>
                <exclude>**/Abstract*TestCase.java</exclude>
                <exclude>**/renderer/v2/**/*.*</exclude>
            </excludes>
            <resources>
                <resource>
                    <directory>src/test/etc</directory>
                    <includes>**/*.*</includes>
                </resource>
                <resource>
                    <directory>src/etc/java</directory>
                    <includes>**/*.*</includes>
                </resource>
            </resources>
        </unitTest>

    </build>
    <reports>
        <!--        <report>maven-pmd-plugin</report>-->
        <!--        <report>maven-simian-plugin</report>-->
        <!--        <report>maven-jdepend-plugin</report>-->
        <!--        <report>maven-jxr-plugin</report>-->
        <!--        <report>maven-javadoc-plugin</report>-->
        <!--        <report>maven-changes-plugin</report>-->
        <!-- The Clover report MUST be before the JUnit report as the test:test
        target is run only ONCE (due to the test:test postGoal) and clover needs to
        "watch" the tests run, while JUnit report simply analyses the output from
        the tests
        -->
        <report>maven-clover-plugin</report>
        <report>maven-junit-report-plugin</report>
    </reports>
